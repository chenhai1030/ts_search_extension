{
  "manifest_version": 2,
  "name": "Funtv Gallery",
  "description": "Gallery Extension!",
  "version": "1.32",
  "browser_action": {
    "default_title": "Funtv Gallery"
  },

  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },

  "permissions": [
    "tabs",
    "contextMenus",
    "webRequest", 
    "webRequestBlocking",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "web_accessible_resources":[
    "js/upload_cover_inject.js",
    "clipper.html",
    "helper.html"
  ],
  "icons": {
      "16": "images/get_started16.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
}